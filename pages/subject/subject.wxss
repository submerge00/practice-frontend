/* pages/paper/paper.wxss */
button{
  color: rgb(32, 107, 245);
  width: 25%;
  height: 45px;
  font-size: 16px;
  background-color: rgb(255, 255, 255);
  border-radius: 15px;
}
.item-text{
  margin-left: 40rpx;
  color: rgb(29, 29, 29);
  width: 60%;
  height:45px;
  text-align: center;
  font-size: 20px;
  line-height: 45px;
  border-radius: 10px;
  background-color: rgb(247, 247, 247);
  border:1px dashed lightgray;
}
.button-text{
  box-shadow: 5px 5px 5px 1px rgb(179, 194, 211);
  width: 95%;
  margin: 20px auto;
  height: 60px;
  display: flex;
  padding-top: 5%;
  border-radius: 20px;
  background-color: rgb(245, 245, 245);
}
.box{
  color: rgb(255, 255, 255);
  background-color: rgb(83, 128, 253);
  text-align: center;
  padding-top: 25px;
  font-size: 25px;
  width: 100%;
  height: 20%;            
  position: absolute;
  border-bottom-left-radius:50rpx ;
  border-bottom-right-radius:50rpx ;
  top: 0;
  left: 0;
}
.container{
  width: 100vw;
  height: 100vh;
  position: relative;
}
.stack-top{
  z-index: 9;
  margin: 0 auto;
  padding-top: 10px;
  width: 90%;
  height: 83%;
  background-color: white;
  border-radius:30px ;
  box-shadow: 5px 5px 15px 5px rgba(180, 180, 180, 0.425);
  position: absolute;
  top:80px;
  overflow:scroll;
}
.example-search {
  background-color: var(--bg-color-demo);
  padding: 16rpx 32rpx;
}

.example-search:not(:last-child) {
  margin-bottom: 32rpx;
}